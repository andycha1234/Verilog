// implements a 2-bit wide 4-to-1 multiplexer
module mux_3bit_8to1 (S, R, T, U, V, W, X, Y, Z, M);
input [2:0] S, U, V, W, X;
output reg [2:0] M;

always@(R or T or U or V or W or X or Y or Z or S)
begin
	case(S)
		3'b000: M <= R;
		3'b001: M <= T;
		3'b010: M <= U;
		3'b011: M <= V;
		3'b100: M <= W;
		3'b101: M <= X;
		3'b110: M <= Y;
		3'b111: M <= Z;
	endcase
end

endmodule